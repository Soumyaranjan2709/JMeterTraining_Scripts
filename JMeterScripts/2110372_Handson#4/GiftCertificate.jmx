              <stringProp name="script">import java.util.Random;
// Get the correlated radio button value
String theme = vars.get(&quot;C_voucher_theme_id&quot;);
// Initialize random generator
Random rand = new Random();
String message = &quot;&quot;;
// Select a random message based on the theme
if (&quot;Christmas&quot;.equals(theme)) {
   String[] messages = vars.get(&quot;ChristmasMessages&quot;).split(&quot;,&quot;);
   message = messages[rand.nextInt(messages.length)];
} else if (&quot;Birthday&quot;.equals(theme)) {
   String[] messages = vars.get(&quot;BirthdayMessages&quot;).split(&quot;,&quot;);
   message = messages[rand.nextInt(messages.length)];
} else if (&quot;General&quot;.equals(theme)) {
   String[] messages = vars.get(&quot;GeneralMessages&quot;).split(&quot;,&quot;);
   message = messages[rand.nextInt(messages.length)];
}
// Set the message variable
vars.put(&quot;SelectedMessage&quot;, message);</stringProp>
