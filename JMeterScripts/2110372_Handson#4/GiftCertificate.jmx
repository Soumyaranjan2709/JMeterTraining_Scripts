	def randomvoucher_id = vars.get(&quot;C_voucher_theme_id_&quot; + randomIndex)

	vars.put(&quot;randomvoucher_id&quot;, randomvoucher_id)

	log.info(&quot;Selected Random voucher ID: &quot; + randomvoucher_id)
}
else
{
